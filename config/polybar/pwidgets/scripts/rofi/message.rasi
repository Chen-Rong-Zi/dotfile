/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @bg;
    text-color:             @fg;
	font:				 	"Iosevka Nerd Font 18";
}

window {
    width:      			400;
    padding:    			20px;
    border:					0px;
    border-radius:          20px;
    border-color:           @ac;
    location:               center;
    x-offset:               0px;
    y-offset:               0px;
}

entry {
    expand: 				true;
    text-color:				@ac;
}
